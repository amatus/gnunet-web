PROJECT="gnunet"

TOOLDIR="toolchain"
export TOOLDIR

PATH="${SYSROOT}/${TOOLDIR}/bin:${SYSROOT}/${TOOLDIR}/opt/emscripten:${PATH}"
export PATH

EM_CONFIG="${SYSROOT}/${TOOLDIR}/etc/emscripten"
export EM_CONFIG

EM_CACHE="${SYSROOT}/${TOOLDIR}/var/cache/emscripten"
export EM_CACHE

